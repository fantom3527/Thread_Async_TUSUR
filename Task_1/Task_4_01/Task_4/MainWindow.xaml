        xmlns:local="clr-namespace:Task_4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Grid.Column="1" Content="Загрузка" Margin="65,29,58,0" VerticalAlignment="Top" Height="35" Click="btnDownload_Click"/>
        <Button Grid.Column="1" Content="Проверка" Margin="65,94,57,0" VerticalAlignment="Top" Height="35" Click="btnWarning_Click"/>
        <TextBox Grid.Column="0" x:Name="richTextBox" Margin="28,0,33,0" Text="" TextWrapping="Wrap" VerticalAlignment="Center" Height="396"/>
    </Grid>
</Window>
